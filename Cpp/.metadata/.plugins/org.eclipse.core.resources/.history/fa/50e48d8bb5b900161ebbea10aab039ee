#include <iostream>
using namespace std;


class Test {

	void test() {
		cout << "from class" << endl;
	}
};

int main() {
	cout << "!!!Hello World!!!" << endl; // prints !!!Hello World!!!

	Test test = new Test();
	test.test();

	return 0;
}
